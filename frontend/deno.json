{
  "tasks": {
    "dev": "deno run -A npm:vite --host 0.0.0.0 --port 3001",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "type-check": "deno check src/**/*.ts src/**/*.tsx",
    "test": "deno run -A npm:vitest",
    "test:ui": "deno run -A npm:vitest --ui",
    "test:coverage": "deno run -A npm:vitest --coverage"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any", "no-unused-vars", "require-await", "no-sloppy-imports", "no-window"]
    }
  },
  "unstable": [
    "sloppy-imports"
  ],
  "nodeModulesDir": "auto",
  "imports": {
    "@/": "./src/",
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.3",
    "@radix-ui/react-checkbox": "npm:@radix-ui/react-checkbox@^1.3.3",
    "@radix-ui/react-collapsible": "npm:@radix-ui/react-collapsible@^1.1.3",
    "@radix-ui/react-dialog": "npm:@radix-ui/react-dialog@^1.1.15",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.6",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.2",
    "@radix-ui/react-menubar": "npm:@radix-ui/react-menubar@^1.1.6",
    "@radix-ui/react-popover": "npm:@radix-ui/react-popover@^1.1.15",
    "@radix-ui/react-scroll-area": "npm:@radix-ui/react-scroll-area@^1.2.10",
    "@radix-ui/react-separator": "npm:@radix-ui/react-separator@^1.1.7",
    "@radix-ui/react-slider": "npm:@radix-ui/react-slider@^1.2.3",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.2.3",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.1.8",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.59.20",
    "@std/path": "jsr:@std/path@^1.0.8",
    "bson": "npm:bson@^6.10.4",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "cmdk": "npm:cmdk@^1.1.1",
    "d3": "npm:d3@^7.9.0",
    "frimousse": "npm:frimousse@^0.3.0",
    "lodash": "npm:lodash@^4.17.21",
    "lucide-react": "npm:lucide-react@^0.475.0",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-hook-form": "npm:react-hook-form@^7.63.0",
    "@hookform/resolvers": "npm:@hookform/resolvers@^5.2.2",
    "react-router-dom": "npm:react-router-dom@^7.4.0",
    "react-resizable-panels": "npm:react-resizable-panels@^2.1.7",
    "crossfilter2": "npm:crossfilter2@^1.5.4",
    "@remix-run/react": "npm:@remix-run/react@^2.16.5",
    "tailwind-merge": "npm:tailwind-merge@^3.3.1",
    "tailwindcss-animate": "npm:tailwindcss-animate@^1.0.7",
    "zod": "npm:zod@^3.24.1",
    "zustand": "npm:zustand@^5.0.2"
  },
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "deno.window"
    ],
    "types": [
      "vite/client",
      "@types/node"
    ],
    "jsx": "react-jsx",
    "strict": true
  }
}


