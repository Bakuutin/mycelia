{
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any", "no-unused-vars", "require-await"]
    }
  },
  "unstable": [
    "sloppy-imports"
  ],
  "nodeModulesDir": "auto",
  "imports": {
    "@/": "./app/",
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.7",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7",
    "@needle-di/core": "jsr:@needle-di/core@^1.0.0",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
    "@std/expect": "jsr:@std/expect@^1.0.16",
    "@std/net": "jsr:@std/net@^1.0.4",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "@std/uuid": "jsr:@std/uuid@^1.0.5",
    "@zaubrik/djwt": "jsr:@zaubrik/djwt@^3.0.2",
    "ms": "npm:ms",
    "react": "npm:react@^18.3.1",
    "bullmq": "npm:bullmq@^5.1.1",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.540.0",
    "@aws-sdk/s3-request-presigner": "npm:@aws-sdk/s3-request-presigner@^3.540.0",
    "formdata-node": "npm:formdata-node@^6.0.0"
  },
  "compilerOptions": {
    "lib": [
      "DOM",
      "deno.window"
    ],
    "types": [
      "@remix-run/node",
      "vite/client"
    ],
    "jsx": "react",
    "strict": true
  }
}
